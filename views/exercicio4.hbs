<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta http-equiv="X-UA-Compatible" content="IE=edge" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>{{titulo}}</title>
    </head>
    <di>

        <header>
            <h1>{{titulo}}</h1>
            <h3>Calcular custo final do consumidor</h3>
        </header>

        <form method="post" action="">
            <label for="precoFabrica">Custo de fábrica:</label>
            <input type="number" id="precoFabrica" name="precoFabrica" min="0"/>
            <button type="submit">Calcular</button>
        </form>

        {{#if precoFinal}}
        <p>Custo final é R${{precoFinal}}</p>
        {{/if}}

        <a href="/">Voltar a pagina principal</a>
    </body>
</html>